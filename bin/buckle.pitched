#!/bin/bash

# example usage:
#  setsid buckle.pitched pitch 2000

tmpdir=$(mktemp -d);
echo "Files stored at: ${tmpdir}"
for file in /usr/share/bucklespring/*.wav; do
  filename=$(basename $file);
  sox "$file" "$tmpdir/$filename" $@
done;
# buckle -p "$tmpdir" -g 75 &>/dev/null
buckle -p "$tmpdir" &>/dev/null
