  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch C++ CMake Project",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/a.out",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "preLaunchTask": "CMake: build",
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "logging": {
        "moduleLoad": false,
        "trace": true
      }
    }
  ]
}

